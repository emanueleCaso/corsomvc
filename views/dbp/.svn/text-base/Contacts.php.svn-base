namespace views\dbp;

use framework\View;

class Contacts extends View
{

    /**
    * Object constructor.
    *
    * @param string|null $tplName The html template containing the static design.
    */
    public function __construct($tplName = null)
    {
        if (empty($tplName))
            $tplName = "/contacts";
        parent::__construct($tplName);
    }
    
}
